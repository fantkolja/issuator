{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"moduleResolution": "node",
		"pretty": true,
		"sourceMap": true,
		"outDir": "dist",
		"importHelpers": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"baseUrl": "./",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"types": [
			"node"
		]
  },
  "exclude": [
	  "frontend",
	  "test"
  ]
}
