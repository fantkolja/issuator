{
  "verbose": false,
  "ignore": [
    ".git",
    "node_modules",
    "frontend"
  ],
  "ext": "ts json",
  "execMap": {
    "ts": "node"
  },
  "events": {
    "start": "tslint -c ./tslint.json -t stylish 'src/**/*.ts'"
  },
  "env": {
    "NODE_ENV": "development",
    "API_PORT": 5000,
    "API_TEST_PORT": 5001,
    "API_URL": "http://localhost"
  }
}
